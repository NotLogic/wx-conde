/* 引入外部wxss */
@import "/wxParse/wxParse.wxss";

/**app.wxss**/
.container {
  font-size: 16px;
  color: #333;
} 
.fr{
  float: right;
}
.fl{
  float: lefht;
}
/* 对富文本内容进行优化展示 .markdown-text 富文本容器 */
.markdown-text{
  
}
.markdown-text img{
  display: none;
}
.markdown-text ul,
.markdown-text ol,
.markdown-text li{
  list-style: none;
}
.markdown-text a{
  color: #19be6b;
  text-decoration: underline;
}

/*  */
