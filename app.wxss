  padding: 30rpx;
  border-bottom: 1rpx solid #efefef;
}

.sub-title{
  font-size: 14px;
}

.top{
  color: #fff;
  border-radius: 5rpx;
  margin-left: 15rpx;
  background-color: #19be6b;
}
.good{
  color: #ed3f14;
  border-radius: 5rpx;
  margin-left: 15rpx;
  border: 1rpx solid #ed3f14;
}

